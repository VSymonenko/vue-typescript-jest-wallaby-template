{
  "name": "frontend",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "yarn test && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-carousel": "vue-cli-service test:unit --watch ./tests/unit/components/carousel3D.spec.ts",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:jest-update": "jest -u",
    "test:jest-clear": "jest --clearCache"
  },
  "dependencies": {
    "@vue/babel-preset-app": "^3.6.0",
    "axios": "^0.18.0",
    "moment": "^2.24.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "register-service-worker": "^1.5.2",
    "v-lazy-image": "^1.3.2",
    "vue": "^2.6.10",
    "vue-awesome-swiper": "^3.1.3",
    "vue-class-component": "^6.0.0",
    "vue-i18n": "^8.9.0",
    "vue-lazyload": "^1.2.6",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.12",
    "vuex": "^3.0.1",
    "vuex-class-component": "^1.4.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@types/jest": "^24.0.11",
    "@types/swiper": "^4.4.2",
    "@vue/cli-plugin-e2e-nightwatch": "^3.5.1",
    "@vue/cli-plugin-pwa": "^3.4.0",
    "@vue/cli-plugin-typescript": "^3.4.0",
    "@vue/cli-plugin-unit-jest": "^3.5.1",
    "@vue/cli-service": "^3.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "axios-mock-adapter": "^1.16.0",
    "chromedriver": "^2.46.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-transform-css": "^2.0.0",
    "lint-staged": "^8.1.5",
    "nightwatch": "^1.0.19",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.4.4",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2",
    "vuex-module-decorators": "^0.9.8",
    "wallaby-vue-compiler": "^1.0.5"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "vue-cli-service lint --no-fix",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint --no-fix",
      "git add"
    ]
  }
}
